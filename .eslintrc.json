{
    "env": {
        "browser": true,
        "es6": true
    },
	"plugins": ["jasmine"],
    "extends": "eslint:recommended",
    "parserOptions": {
		"ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
		"no-console": 0
    }
}